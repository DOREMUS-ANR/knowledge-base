    skos:prefLabel "mime"@en,
        "mime"@fr,
        "mimo"@it ;
    skos:topConceptOf <http://data.doremus.org/vocabulary/function/> .

<http://data.doremus.org/vocabulary/function/musician_(performer)> a skos:Concept ;
    skos:definition "Agent qui exécute ou dirige de la musique."@fr ;
    skos:prefLabel "musician (performer)"@en,
        "musicien (interprète)"@fr,
        "musicista (interprete)"@it ;
    skos:topConceptOf <http://data.doremus.org/vocabulary/function/> .

<http://data.doremus.org/vocabulary/function/narrator> a skos:Concept ;
    skos:altLabel "lecteur"@fr ;
    skos:definition "Agent lisant à voix haute ou\\nrendant compte d’une action, d’une situation, du déroulement des événements, etc. (RDA, modifié)"@fr ;
