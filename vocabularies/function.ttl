    skos:prefLabel "sound-effects engineer"@en,
        "bruiteur"@fr,
        "rumorista"@it ;
    skos:topConceptOf <http://data.doremus.org/vocabulary/function/> .

<http://data.doremus.org/vocabulary/function/sound_engineering_technician> a skos:Concept ;
