<http://data.doremus.org/vocabulary/function/extra> a skos:Concept ;
    skos:altLabel "background actor"@en,
        "comparsa"@it ;
    skos:broader <http://data.doremus.org/vocabulary/function/actor> ;
    skos:definition "Acteur à qui l'on ne demande que\\nla présence dans une pièce de théâtre, \\nun film, un ballet, etc. \\n(Larousse) \\n"@fr ;
    skos:inScheme <http://data.doremus.org/vocabulary/function/> ;
    skos:prefLabel "extra"@en,
        "figurant"@fr,
        "figurante"@it ;
    skos:scopeNote "Dans la distribution, la mention de cette fonction est un raccourci pour l'exécution d'une oeuvre de spectacle vivant incorporée à une oeuvre musicale."@fr .

<http://data.doremus.org/vocabulary/function/imitator> a skos:Concept ;
    skos:definition "Agent qui imite le comportement, les \\ngestes, les paroles d'autrui dans le cadre d'un spectacle. \\n(Centre National de Ressources Textuelles \\net Lexicales CNRTL, modifié)"@fr ;
    skos:prefLabel "imitator"@en,
        "imitateur"@fr,
        "imitatore"@it ;
    skos:topConceptOf <http://data.doremus.org/vocabulary/function/> .

<http://data.doremus.org/vocabulary/function/juggler> a skos:Concept ;
    skos:definition "Agent qui lance en l'air divers objets qu'il reçoit et relance alternativement en entrecroisant leur trajectoire. \\n(Le Petit Robert, modifié)"@fr ;
    skos:prefLabel "juggler"@en,
        "jongleur"@fr,
        "giocoliere"@it ;
    skos:topConceptOf <http://data.doremus.org/vocabulary/function/> .

<http://data.doremus.org/vocabulary/function/librettist> a skos:Concept ;
    skos:broader <http://data.doremus.org/vocabulary/function/author> ;
    skos:definition "Auteur des paroles d’un opéra, d’un oratorio, ou d’une autre œuvre lyrique, à l'exception de la comédie musicale."@fr ;
    skos:inScheme <http://data.doremus.org/vocabulary/function/> ;
    skos:prefLabel "librettist"@en,
        "librettiste"@fr,
        "librettista"@it ;
    skos:scopeNote "A associer à l'œuvre textuelle incorporée dans l'œuvre musicale."@fr .

<http://data.doremus.org/vocabulary/function/lyricist> a skos:Concept ;
    skos:altLabel "auteur des lyrics"@fr,
        "auteur parolier"@fr,
        "lyriciste"@fr ;
    skos:broader <http://data.doremus.org/vocabulary/function/author> ;
    skos:definition "Auteur des paroles d’une chanson ou d’une comédie musicale."@fr ;
    skos:inScheme <http://data.doremus.org/vocabulary/function/> ;
    skos:prefLabel "lyricist"@en,
        "parolier"@fr,
        "paroliere"@it ;
    skos:scopeNote "A associer à l'œuvre textuelle incorporée dans l'œuvre musicale."@fr .

<http://data.doremus.org/vocabulary/function/mime> a skos:Concept ;
