    skos:definition "Not printed or manuscript music."@en ;
    skos:inScheme <http://iflastandards.info/ns/unimarc/terms/tos#> ;
    skos:notation "x"@en ;
    skos:prefLabel "not applicable"@en ;
    skos:topConceptOf <http://iflastandards.info/ns/unimarc/terms/tos#> .

<http://iflastandards.info/ns/unimarc/terms/tos#z> a skos:Concept ;
