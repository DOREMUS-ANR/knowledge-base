<http://data.doremus.org/period/french_revolution> a ecrm:E4_Period,
        time:ProperInterval ;
    rdfs:label "French Revolution"@en,
        "Révolution française"@fr,
        "Rivoluzione francese"@it ;
    ecrm:P1_is_identified_by "French Revolution"@en,
        "Révolution française"@fr,
        "Rivoluzione Francese"@it ;
    rdfs:comment "The French Revolution was a period of far-reaching social and political upheaval in France and its colonies that lasted from 1789 until 1799."@en ;
    time:hasBeginning [ a time:Instant ;
            time:inXSDDate "1789-05-05"^^xsd:date ] ;
    time:hasEnd [ a time:Instant ;
            time:inXSDDate "1799-11-09"^^xsd:date ] ;
    time:intervalIn <http://data.doremus.org/period/18_century> .

<http://data.doremus.org/period/renaissance> a ecrm:E4_Period,
        time:ProperInterval ;
    rdfs:label "Renaissance"@en,
